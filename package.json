{
  "name": "weekly-forecast",
  "version": "1.0.0",
  "description": "Front end weekly forecast for a given location and proxy server",
  "main": "index.js",
  "scripts": {
    "client": "cd client && npm run start",
    "server": "cd cors-anywhere && node server",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "setup:client": "cd client && npm i",
    "setup:server": "cd cors-anywhere && npm i",
    "setup": "concurrently \"npm run setup:client\" \"npm run setup:server\"",
    "test:client": "cd client && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LadCoder/weekly-forecast.git"
  },
  "author": "Coderlad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/LadCoder/weekly-forecast/issues"
  },
  "homepage": "https://github.com/LadCoder/weekly-forecast#readme",
  "dependencies": {
    "concurrently": "^6.2.2"
  }
}
